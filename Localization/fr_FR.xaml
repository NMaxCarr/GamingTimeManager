<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Settings -->
    
    <sys:String x:Key="LOCGamingTimeManagerTimeLimitMin">Limite de temps (minutes) :</sys:String>
    <sys:String x:Key="LOCGamingTimeManagerPeriod">Période :</sys:String>
    <sys:String x:Key="LOCGamingTimeManagerWarnTimeOver">M'avertir lorsque le temps est écoulé</sys:String>

    <!-- Alarm notification -->

    <sys:String x:Key="LOCGamingTimeManagerTimeOver">Temps écoulé</sys:String>
    <sys:String x:Key="LOCGamingTimeManagerMaxTimeAllowed">Vous avez joué votre temps de jeu maximal autorisé</sys:String>

    <!-- Summary notification -->

    <sys:String x:Key="LOCGamingTimeManagerSummary">Vous avez joué {0} de votre limite {1}.</sys:String>

</ResourceDictionary>
